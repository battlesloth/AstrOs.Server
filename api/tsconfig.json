{
  "compilerOptions": {
      "module": "commonjs",
      "allowJs": true,
      "esModuleInterop": true,
      "target": "es6",
      "noImplicitAny": true,
      "moduleResolution": "node",
      "sourceMap": true,
      "outDir": "dist",
      "baseUrl": ".",
      "strict": true,
      "forceConsistentCasingInFileNames": true,
      "importHelpers": true,
      "paths": {
          "*": [
              "node_modules/*"
          ]
      }
  }
}